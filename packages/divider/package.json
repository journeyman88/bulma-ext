{
  "name": "@bulma-ext/divider",
  "version": "1.0.0",
  "description": "A Divider Stylesheet compatible with Bulma 1.0.x (https://bulma.io)",
  "license": "Apache-2.0",
  "main": "css/divider.min.css",
  "sass": "sass/divider.scss",
  "scripts": {
    "build:expanded": "sass sass/divider.scss css/divider.css --load-path=node_modules --quiet-deps --source-map",
    "build:min": "sass sass/divider.scss css/divider.min.css --load-path=node_modules --quiet-deps --style=compressed --no-source-map",
    "build:demo": "astro build",
    "build": "pnpm run build:expanded && pnpm run build:min && pnpm build:demo",
    "watch": "sass sass/divider.scss css/divider.css --watch"
  },
  "keywords": [
    "bulma",
    "bulma-extension"
  ],
  "peerDependencies": {
    "bulma": "^1.0.0"
  },
  "devDependencies": {
    "sass": "^1.77.0",
    "astro": "^5.17.2"
  }
}
