{
  "name": "@bulma-ext/timeline",
  "version": "1.0.0",
  "description": "A Timeline Stylesheet compatible with Bulma 1.0.x (https://bulma.io)",
  "license": "Apache-2.0",
  "main": "css/timeline.min.css",
  "sass": "sass/timeline.scss",
  "scripts": {
    "build:expanded": "sass sass/timeline.scss css/timeline.css --load-path=node_modules --quiet-deps --source-map",
    "build:min": "sass sass/timeline.scss css/timeline.min.css --load-path=node_modules --quiet-deps --style=compressed --no-source-map",
        "build:demo": "astro build",
    "build": "pnpm run build:expanded && pnpm run build:min && pnpm build:demo",
    "watch": "sass sass/timeline.scss css/timeline.css --watch"
  },
  "keywords": [
    "bulma",
    "bulma-extension"
  ],
  "peerDependencies": {
    "bulma": "^1.0.0"
  },
  "devDependencies": {
    "sass": "^1.77.0",
        "astro": "^5.17.2"
  }
}
